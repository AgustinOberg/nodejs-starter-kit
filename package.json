{
  "name": "nodejs-starter",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "coverage": "nyc --reporter=text npm run test",
    "deploy:development": "NODE_ENV=development npm run internal-deploy",
    "deploy:production": "NODE_ENV=production npm run internal-deploy",
    "dev": "NODE_ENV=local nodemon --exec babel-node ./app.js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "internal-babel-transpile": "rm -rf dist && babel . -d dist --ignore node_modules,test --copy-files --no-copy-ignored",
    "internal-build": "npm run internal-babel-transpile && npm run internal-copy-modules && npm run internal-merge-env",
    "internal-copy-modules": "rm -rf dist/node_modules && rm -rf dist/migrations && cd dist && npm install --only=prod && rm -rf .git && cd ..",
    "internal-deploy": "npm run internal-build && serverless deploy --stage=$NODE_ENV",
    "internal-merge-env": "cp .env.$NODE_ENV dist/.env",
    "internal-remove": "serverless remove --stage=$NODE_ENV",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "remove:development": "NODE_ENV=development npm run internal-remove",
    "remove:production": "NODE_ENV=production npm run internal-remove",
    "start": "babel-node app.js",
    "test": "env-cmd -f ./.env.test mocha --timeout 60000 --recursive --require @babel/register --exit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "chai": "^4.3.7",
    "env-cmd": "^10.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "faker": "^5.5.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "serverless-dotenv-plugin": "^6.0.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.22.6",
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "http-status-codes": "^2.2.0",
    "mongoose": "^7.3.4",
    "serverless-http": "^3.2.0"
  }
}
